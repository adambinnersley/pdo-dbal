{
    "name": "adamb7586/database",
    "description": "A simple PDO Wrapper to connect to a database",
    "keywords": ["PDO", "database", "db", "wrapper"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/AdamB7586/pdo-dbal",
    "authors": [
        {
            "name": "Adam Binnersley",
            "email": "abinnersley@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "phpunit/dbunit": "^3.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DBAL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DBAL\\Tests\\": "tests/"
        }
    }
}
